{{!天气插件}}
<div class="weatherSidget">
	{{#each partials.locations}}
		<div class="location">
			<h3>{{name}}</h3>
			<a href="{{forecastUrl}}">
				<img src="{{iconUrl}}" alt="{{weather}}">
				{{weather}},{{temp}}
			</a>
		</div>
	{{/each}}
	<small>source:<a href="http://www.wunderground.com">Weather Umderground</a></small>
</div>